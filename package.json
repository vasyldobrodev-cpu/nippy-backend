{
  "name": "media-freelance-backend",
  "version": "1.0.0",
  "description": "Backend API for media freelancing platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "db_test": "node src/scripts/db_test.js",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "swagger": "ts-node src/scripts/generateSwagger.ts",
    "setup-db": "npx ts-node src/scripts/setupDatabase.ts",
    "db:create": "npx ts-node src/scripts/setupDatabase.ts",
    "db:check": "npx ts-node -e \"import('./src/utils/initializeDb').then(m => m.checkPostgreSQLConnection())\""
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "freelancing",
    "media"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^19.1.9",
    "@types/socket.io": "^3.0.1",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "child_process": "^1.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "fs": "^0.0.1-security",
    "helmet": "^7.2.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "reflect-metadata": "^0.1.14",
    "socket.io": "^4.8.1",
    "stripe": "^14.25.0",
    "typeorm": "^0.3.25",
    "uuid": "^9.0.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
